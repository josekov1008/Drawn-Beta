<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drawn.drawn"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.drawn.drawn.FacebookIntegration"
        android:allowBackup="true"
        android:icon="@drawable/drawnicono"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data android:value="@string/app_id" android:name="com.facebook.sdk.ApplicationId"/>
		    
		    <activity
		        android:name=".MainActivity"
		        android:label="@string/app_name" >
		    </activity>
		    <activity
		        android:name=".LoginActivity"
		        android:label="@string/app_name" >
		        <intent-filter>
	                <action android:name="android.intent.action.MAIN" />
					<category android:name="android.intent.category.LAUNCHER" />
	            </intent-filter>
		    </activity>
		    <activity
		        android:name=".ReceiveActivity"
		        android:label="@string/app_name" >
		    </activity>
		    <activity
		        android:name=".DrawingListActivity"
		        android:label="@string/app_name" >
		    </activity>
		    <activity
		        android:name=".SentActivity"
		        android:label="@string/app_name" >
		    </activity>
		    <activity
            android:name="com.facebook.LoginActivity"
            android:label="@string/app_name">
        </activity>
    
		    <activity android:name="com.drawn.drawn.PickFriendsActivity" > </activity>
    
    </application>
    
    

</manifest>
